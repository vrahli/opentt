if-equalInType-EQ-test u w T a b t‚ÇÅ t‚ÇÇ (EQTBAR x) eqi =
  inOpenBar-idem
    (Œª w1 e1 ‚Üí
      fst (eqi w1 e1 (fst (x w1 e1)) (‚äë-refl¬∑ _)) ,
      ‚äë-trans¬∑ (fst (snd (x w1 e1))) (fst (snd (eqi w1 e1 (fst (x w1 e1)) (‚äë-refl¬∑ _)))) ,
      Œª w4 e4 z ‚Üí aw w4 z (snd (snd (x w1 e1)) w4 (‚äë-trans¬∑ (‚äë-refl¬∑ _) (‚äë-trans¬∑ (fst (snd (eqi w1 e1 (fst (x w1 e1)) (‚äë-refl¬∑ _)))) e4)) z) (snd (snd (eqi w1 e1 (fst (x w1 e1)) (‚äë-refl¬∑ _))) w4 e4 (‚äë-trans¬∑ (‚äë-refl¬∑ _) (‚äë-trans¬∑ (fst (snd (eqi w1 e1 (fst (x w1 e1)) (‚äë-refl¬∑ _)))) e4)) z))
  where
    aw : ‚àÄùïé w
              (Œª w' e' ‚Üí
                (x‚ÇÅ : eqTypes (uni u) w' (#EQ a b T) (#EQ a b T))
                ‚Üí eqInType (uni u) w' x‚ÇÅ t‚ÇÅ t‚ÇÇ
                ‚Üí ‚ñ°¬∑ w' (‚ÜëwPred' (Œª w'' e ‚Üí equalInType u w'' T a b) e'))
    aw w1 e1 eqt1 eqi1 =
      Œª w1 e1 ‚Üí
        fst (h w1 e1) ,
        ‚äë-trans¬∑ (fst (snd (h w1 e1))) (‚äë-refl¬∑ (fst (h w1 e1))) ,
        Œª w4 e4 z z‚ÇÄ ‚Üí snd (snd (h w1 e1)) w4 (‚äë-trans¬∑ (‚äë-refl¬∑ (fst (h w1 e1))) e4) z
      where
        h : inOpenBar w1 (Œª w' e' ‚Üí equalInType u w' T a b)
        h = if-equalInType-EQ-test u w1 T a b t‚ÇÅ t‚ÇÇ eqt1 eqi1
--}


