  where
    c1 : sub ‚åú b ‚åù (LET (VAR 0) (APPLY (shiftUp 0 (shiftUp 0 ‚åú R ‚åù)) (APPEND (shiftUp 0 (shiftUp 0 ‚åú l ‚åù)) (VAR 0))))
