        | #shiftUp 0 (ct l cl)
        | #shiftUp 0 (ct l cl)
        | #shiftUp 0 (ct l cl)
        | #shiftUp 0 (ct l cl)
        | #shiftUp 0 (ct l cl)
        | #shiftUp 0 (ct l cl)
        | #shiftUp 0 (ct l cl)
        | #shiftDown 2 (ct l cl)
        | #shiftDown 3 (ct l cl)
        | #shiftDown 5 (ct l cl)
        | #shiftDown 6 (ct l cl)
        | #subv 1 l F cF
        | #subv 8 l F cF
        | #shiftDown 1 (ct F cF)
        | #shiftDown 8 (ct F cF)
        | #shiftUp 1 (ct l cl)
        | #shiftUp 2 (ct l cl)
        | #shiftUp 3 (ct l cl)
        | #shiftUp 4 (ct l cl)
        | #shiftUp 5 (ct l cl)
        | #shiftDown 4 (ct l cl)
