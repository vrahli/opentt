      d1 = #‚áõ-trans {w} {a} {#APPLY2 (#loop r F) j f} {#ETA (#NUM k)} (#‚áõ!‚Üí#‚áõ {w} {a} {#APPLY2 (#loop r F) j f} c1) x

      d2 : b #‚áõ #ETA (#NUM k) at w
      d2 = #‚áõ-trans {w} {b} {#APPLY2 (#loop r F) j f} {#ETA (#NUM k)} (#‚áõ!‚Üí#‚áõ {w} {b} {#APPLY2 (#loop r F) j f} c2) x

      eqb : (b1 b2 : CTerm)
            ‚Üí equalInType i w (sub0 (#INL (#NUM k)) #IndBarC) b1 b2
            ‚Üí meq (equalInType i w #IndBarB) (Œª a b eqa ‚Üí equalInType i w (sub0 a #IndBarC))
                   w (#APPLY #AX b1) (#APPLY #AX b2)
      eqb b1 b2 eb rewrite sub0-IndBarC‚â° (#INL (#NUM k)) = ‚ä•-elim (equalInType-DECIDE-INL-VOID‚Üí i w (#NUM k) b1 b2 #[0]NAT! eb)
... | inj‚ÇÇ x =
  #INR #AX  , #loopR (#loop r F) j f , #INR #AX , #loopR (#loop r F) j f , INR‚ààIndBarB i w ,
  #‚áõ-trans {w} {a} {#APPLY2 (#loop r F) j f} {#DIGAMMA (#loopR (#loop r F) j f)} (#‚áõ!‚Üí#‚áõ {w} {a} {#APPLY2 (#loop r F) j f} c1) x ,
  #‚áõ-trans {w} {b} {#APPLY2 (#loop r F) j f} {#DIGAMMA (#loopR (#loop r F) j f)} (#‚áõ!‚Üí#‚áõ {w} {b} {#APPLY2 (#loop r F) j f} c2) x ,
  eqb
    where
      eqb : (b1 b2 : CTerm)
            ‚Üí equalInType i w (sub0 (#INR #AX) #IndBarC) b1 b2
            ‚Üí meq (equalInType i w #IndBarB) (Œª a b eqa ‚Üí equalInType i w (sub0 a #IndBarC))
                   w (#APPLY (#loopR (#loop r F) j f) b1) (#APPLY (#loopR (#loop r F) j f) b2)
      eqb b1 b2 eb rewrite sub0-IndBarC‚â° (#INR #AX) = eb3
        where
