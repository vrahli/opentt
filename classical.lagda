    {#UNIV i} {#[0]SQUASH (#[0]UNION (#[0]‚ÜëT p #[0]VAR) (#[0]NEG (#[0]‚ÜëT p #[0]VAR)))}
    {#UNIV i} {#[0]SQUASH (#[0]UNION (#[0]‚ÜëT p #[0]VAR) (#[0]NEG (#[0]‚ÜëT p #[0]VAR)))}
