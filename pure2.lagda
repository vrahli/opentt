--
-- This lemma was suggested by Yannick Forster.
--
-- MP‚Çö·µ§·µ£‚Çë : œÄ (F : (‚Ñï ‚Üí ùîπ) ‚à© pure). ¬¨(Œ† (n : ‚Ñï). ¬¨(F n ‚â° true)) ‚Üí ||Œ£ (n : ‚Ñï). F n ‚â° true||
-- MP‚Çö·µ£   : œÄ (m : ‚Ñï). ¬¨(Œ† (n : ‚Ñï). ¬¨(eval m n ‚â° true)) ‚Üí ||Œ£ (n : ‚Ñï). eval m n ‚â° true||
--
-- We show MP‚Çö·µ§·µ£‚Çë ‚Üí MP‚Çö·µ£ when eval is a pure function (i.e., it satisfies #¬¨Names) in ‚Ñï ‚Üí ‚Ñï ‚Üí ùîπ
--
-- Very rough proof sketch:
--                 ‚àÄ w' ‚âª w. m ‚àà ‚ñ° Nat @ w' ‚Üí ‚ñ° P at w' w
--              -> use K: ‚àÄ w' ‚âª w. ‚ñ° w' (‚àÄ m‚àà‚Ñï. P)
--              -> use K on MP_pure
--              -> instantiate F with (eval m)
--
-- All datatypes are "no-reads/no-writes" types, so effects are constraints, but still potentially effectful
-- as inhabitants don't have to be pure
--
Œ†pure‚Üí : (i : ‚Ñï) (w : ùïé¬∑) (eval a : CTerm)
          ‚Üí #¬¨Names eval
          ‚Üí #¬¨Enc eval
          ‚Üí ‚ààType i w (#FUN #NAT! #NAT!‚ÜíBOOL‚ÇÄ!) eval
          ‚Üí ‚ààType i w (#PI (#TPURE #NAT!‚ÜíBOOL‚ÇÄ!) (#[0]FUN #[0]MP-left-qt‚ÇÉ #[0]MP-right-qt‚ÇÉ)) a
          ‚Üí ‚ààType i w (#PI #NAT! (#[0]FUN (#[0]MP-left-qt‚ÇÑ eval) (#[0]MP-right-qt‚ÇÑ eval))) a
Œ†pure‚Üí i w eval a nnf nef eval‚àà a‚àà =
  equalInType-PI
    (Œª w' e' ‚Üí isTypeNAT! {w'} {i})
    aw0
    aw1
  where
    aw0 : ‚àÄùïé w (Œª w' _ ‚Üí (a‚ÇÅ a‚ÇÇ : CTerm) ‚Üí equalInType i w' #NAT! a‚ÇÅ a‚ÇÇ
                        ‚Üí equalTypes i w' (sub0 a‚ÇÅ (#[0]FUN (#[0]MP-left-qt‚ÇÑ eval) (#[0]MP-right-qt‚ÇÑ eval)))
                                           (sub0 a‚ÇÇ (#[0]FUN (#[0]MP-left-qt‚ÇÑ eval) (#[0]MP-right-qt‚ÇÑ eval))))
    aw0 w' e a‚ÇÅ a‚ÇÇ a‚àà = ‚â°CTerm‚ÜíeqTypes (sym (sub0-fun-mp-qt‚ÇÑ eval a‚ÇÅ)) (sym (sub0-fun-mp-qt‚ÇÑ eval a‚ÇÇ))
                                         (eqTypesFUN‚Üê (‚ÜíequalTypes-#MP-left-qt‚ÇÉ (equalInType-FUN‚Üí {i} {w} {#NAT!} {#NAT!‚ÜíBOOL‚ÇÄ!} {eval} {eval} eval‚àà w' e a‚ÇÅ a‚ÇÇ a‚àà))
                                                       (‚ÜíequalTypes-#MP-right-qt‚ÇÉ (equalInType-FUN‚Üí {i} {w} {#NAT!} {#NAT!‚ÜíBOOL‚ÇÄ!} {eval} {eval} eval‚àà w' e a‚ÇÅ a‚ÇÇ a‚àà)))

    aw1 : ‚àÄùïé w (Œª w' _ ‚Üí (n‚ÇÅ n‚ÇÇ : CTerm) ‚Üí equalInType i w' #NAT! n‚ÇÅ n‚ÇÇ
                       ‚Üí equalInType i w' (sub0 n‚ÇÅ (#[0]FUN (#[0]MP-left-qt‚ÇÑ eval) (#[0]MP-right-qt‚ÇÑ eval))) (#APPLY a n‚ÇÅ) (#APPLY a n‚ÇÇ))
    aw1 w1 e1 n‚ÇÅ n‚ÇÇ n‚àà = ‚â°CTerm‚ÜíequalInType (sym (sub0-fun-mp-qt‚ÇÑ eval n‚ÇÅ)) (equalInType-local (Mod.‚àÄùïé-‚ñ°Func M aw2 (equalInType-NAT!‚Üí i w1 n‚ÇÅ n‚ÇÇ n‚àà)))
      where
        aw2 : ‚àÄùïé w1 (Œª w' e' ‚Üí #‚áõ!same‚Ñï w' n‚ÇÅ n‚ÇÇ
                              ‚Üí equalInType i w' (#FUN (#MP-left-qt‚ÇÉ (#APPLY eval n‚ÇÅ)) (#MP-right-qt‚ÇÉ (#APPLY eval n‚ÇÅ))) (#APPLY a n‚ÇÅ) (#APPLY a n‚ÇÇ))
        aw2 w2 e2 (n , c‚ÇÅ , c‚ÇÇ) =
          -- the extract doesn't matter, so this is essentially h2, except that we have n‚ÇÅ here and (#NUM n) in h2
          ‚ÜíequalInType-fun-mp-qt‚ÇÉ
            {i} {w2} {#APPLY eval (#NUM n)} {#APPLY eval n‚ÇÅ} {#APPLY a (#APPLY eval (#NUM n))} {#APPLY a (#APPLY eval (#NUM n))}
            (equalInType-FUN‚Üí
              {i} {w} {#NAT!} {#NAT!‚ÜíBOOL‚ÇÄ!} {eval} {eval} eval‚àà w2 (‚äë-trans¬∑ e1 e2) (#NUM n) n‚ÇÅ
              (‚ÜíequalInType-NAT! i w2 (#NUM n) n‚ÇÅ (Mod.‚àÄùïé-‚ñ° M aw3)))
            h2
          where
            aw3 : ‚àÄùïé w2 (Œª w' _ ‚Üí #‚áõ!same‚Ñï w' (#NUM n) n‚ÇÅ)
            aw3 w3 e3 = n , #‚áõ!-refl {w3} {#NUM n} , ‚àÄùïé-mon e3 c‚ÇÅ

            h0 : equalInType i w2 (#TPURE #NAT!‚ÜíBOOL‚ÇÄ!) (#APPLY eval (#NUM n)) (#APPLY eval (#NUM n))
            h0 = ‚ÜíequalInType-TPURE
                   (#¬¨Names-APPLY {eval} {#NUM n} nnf refl)
                   (#¬¨Names-APPLY {eval} {#NUM n} nnf refl)
                   (#¬¨Enc-APPLY {eval} {#NUM n} nef refl)
                   (#¬¨Enc-APPLY {eval} {#NUM n} nef refl)
                   (equalInType-FUN‚Üí {i} {w} {#NAT!} {#NAT!‚ÜíBOOL‚ÇÄ!} {eval} {eval}
                     eval‚àà w2 (‚äë-trans¬∑ e1 e2) (#NUM n) (#NUM n) (NUM-equalInType-NAT! i w2 n))

            h1 : equalInType i w2 (sub0 (#APPLY eval (#NUM n)) (#[0]FUN #[0]MP-left-qt‚ÇÉ #[0]MP-right-qt‚ÇÉ)) (#APPLY a (#APPLY eval (#NUM n))) (#APPLY a (#APPLY eval (#NUM n)))
            h1 = snd (snd (equalInType-PI‚Üí {i} {w} {#TPURE #NAT!‚ÜíBOOL‚ÇÄ!} {#[0]FUN #[0]MP-left-qt‚ÇÉ #[0]MP-right-qt‚ÇÉ} {a} {a} a‚àà))
                     w2 (‚äë-trans¬∑ e1 e2) (#APPLY eval (#NUM n)) (#APPLY eval (#NUM n)) h0

            h2 : equalInType i w2 (#FUN (#MP-left-qt‚ÇÉ (#APPLY eval (#NUM n))) (#MP-right-qt‚ÇÉ (#APPLY eval (#NUM n)))) (#APPLY a (#APPLY eval (#NUM n))) (#APPLY a (#APPLY eval (#NUM n)))
            h2 = ‚â°CTerm‚ÜíequalInType (sub0-fun-mp‚ÇÜ (#APPLY eval (#NUM n))) h1


