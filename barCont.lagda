-- Recursive call used in DIGAMMA
loopR : Term ‚Üí Term ‚Üí Term
loopR R xs = LAMBDA (LET (VAR 0) (APPLY (shiftUp 0 (shiftUp 0 R)) (APPEND (shiftUp 0 (shiftUp 0 xs)) (VAR 0))))


-- loopA's body
loopI : Name ‚Üí Term ‚Üí Term ‚Üí Term ‚Üí Term
loopI r R xs i =
  ITE (get0 r)
      (ETA i)
      (DIGAMMA (loopR R xs))


loopB : Name ‚Üí Term ‚Üí Term ‚Üí Term ‚Üí Term
loopB r a R xs = LET a (loopI r (shiftUp 0 R) (shiftUp 0 xs) (VAR 0))


loopA : Name ‚Üí Term ‚Üí Term ‚Üí Term ‚Üí Term
loopA r bar R xs = loopB r (APPLY bar (generic r xs)) R xs


loopF : Name ‚Üí Term ‚Üí Term ‚Üí Term ‚Üí Term
loopF r bar R xs =
  SEQ (set‚ä§ r) -- we start by assuming that we have enough information
      (loopA r bar R xs)


loopL : Name ‚Üí  Term ‚Üí Term
loopL r bar =
  -- 0 is the argument (the list), and 1 is the recursive call
  LAMBDA (LAMBDA (loopF r bar (VAR 1) (VAR 0)))


loop : Name ‚Üí  Term ‚Üí Term
loop r bar = FIX (loopL r bar)


